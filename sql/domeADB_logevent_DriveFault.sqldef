DROP TABLE IF EXISTS domeADB_logevent_DriveFault;
CREATE TABLE domeADB_logevent_DriveFault (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  driveId int,
  errorCode varchar(32),
  priority int,
  PRIMARY KEY (date_time)
);
