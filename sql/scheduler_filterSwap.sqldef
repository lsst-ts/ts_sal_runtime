DROP TABLE IF EXISTS scheduler_filterSwap;
CREATE TABLE scheduler_filterSwap (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  need_swap tinyint,
  filter_to_unmount varchar(32),
  PRIMARY KEY (date_time)
);
