DROP TABLE IF EXISTS m2ms_TangentActuatorAbsolutePositionSteps;
CREATE TABLE m2ms_TangentActuatorAbsolutePositionSteps (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  tangentLink_0deg_absolutePositionSteps int,
  tangentLink_60deg_absolutePositionSteps int,
  tangentLink_120deg_absolutePositionSteps int,
  tangentLink_180deg_absolutePositionSteps int,
  tangentLink_240deg_absolutePositionSteps int,
  tangentLink_300deg_absolutePositionSteps int,
  PRIMARY KEY (date_time)
);
