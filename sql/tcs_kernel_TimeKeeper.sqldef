DROP TABLE IF EXISTS tcs_kernel_TimeKeeper;
CREATE TABLE tcs_kernel_TimeKeeper (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  Cst float,
  Dcst float,
  Dsst float,
  Sst float,
  Tai float,
  Tt float,
  PRIMARY KEY (date_time)
);
