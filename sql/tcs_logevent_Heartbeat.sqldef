DROP TABLE IF EXISTS tcs_logevent_Heartbeat;
CREATE TABLE tcs_logevent_Heartbeat (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  Heartbeat tinyint,
  priority int,
  PRIMARY KEY (date_time)
);
