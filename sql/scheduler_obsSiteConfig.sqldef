DROP TABLE IF EXISTS scheduler_obsSiteConfig;
CREATE TABLE scheduler_obsSiteConfig (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  name varchar(32),
  latitude double,
  longitude double,
  height double,
  pressure double,
  temperature double,
  relative_humidity double,
  PRIMARY KEY (date_time)
);
