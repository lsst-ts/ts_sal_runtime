DROP TABLE IF EXISTS scheduler_cloud;
CREATE TABLE scheduler_cloud (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  timestamp double,
  cloud double,
  PRIMARY KEY (date_time)
);
