DROP TABLE IF EXISTS hexapod_command_pivot;
CREATE TABLE hexapod_command_pivot (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  x double,
  y double,
  z double,
  PRIMARY KEY (date_time)
);
