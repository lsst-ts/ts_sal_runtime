DROP TABLE IF EXISTS scheduler_program;
CREATE TABLE scheduler_program (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  completion float,
  id int,
  priority int,
  status int,
  PRIMARY KEY (date_time)
);
