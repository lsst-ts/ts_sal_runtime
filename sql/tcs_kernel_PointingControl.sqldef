DROP TABLE IF EXISTS tcs_kernel_PointingControl;
CREATE TABLE tcs_kernel_PointingControl (
  date_time DATETIME(6),
  private_revCode char(32),
  private_sndStamp double precision,
  private_rcvStamp double precision,
  private_seqNum int,
  private_origin int,
  private_host int,
  AGuide float,
  ALocal float,
  BGuide float,
  BLocal float,
  PRIMARY KEY (date_time)
);
